{
  "name": "@ideanick/webapp",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "test": "dotenv -e .env.test -e .env jest",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.node.json",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache 'src/**/*.scss'",
    "stylecheck": "sass --update -q ./src:./node_modules/.cache/.stylecheckcache",
    "prettify": "prettier --loglevel warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' '!**/{node_modules,dist}/**/*'"
  },
  "dependencies": {
    "@ideanick/backend": "workspace:*",
    "@ideanick/shared": "workspace:*",
    "@nanostores/react": "^0.7.1",
    "@sentry/react": "^7.70.0",
    "@tanstack/react-query": "^4.32.6",
    "@trpc/client": "^10.37.1",
    "@trpc/react-query": "^10.37.1",
    "@trpc/server": "^10.37.1",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "formik": "^2.4.4",
    "formik-validator-zod": "^1.0.2",
    "include-media": "^2.0.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "mixpanel-browser": "^2.47.0",
    "nanostores": "^0.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-icons": "^4.11.0",
    "react-image-gallery": "^1.3.0",
    "react-infinite-scroller": "^1.2.6",
    "react-router-dom": "^6.15.0",
    "reset-css": "^5.0.2",
    "sass": "^1.66.1",
    "superjson": "^1.13.1",
    "usehooks-ts": "^2.9.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@sentry/vite-plugin": "^2.23.0",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash": "^4.14.198",
    "@types/mixpanel-browser": "^2.47.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-image-gallery": "^1.2.1",
    "@types/react-infinite-scroller": "^1.2.3",
    "@vitejs/plugin-react": "^4.0.3",
    "prettier": "^3.0.1",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "vite": "^4.4.5",
    "vite-plugin-svgr": "^3.2.0"
  }
}
